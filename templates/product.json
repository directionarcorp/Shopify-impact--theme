            "liquid": "{% if product and product.available %}\n  <!-- On réutilise le même conteneur que le bouton ATC du thème Impact -->\n  <div class=\"product-form__buttons nk-dynamic-checkout-block\">\n    {% form 'product', product, id: 'nk-dynamic-checkout', class: 'nk-dynamic-checkout' %}\n      <input type=\"hidden\" name=\"id\" value=\"{{ product.selected_or_first_available_variant.id }}\">\n      {{ form | payment_button }}\n    {% endform %}\n  </div>\n\n  <style>\n    /* === Forcer la pleine largeur comme le bouton ATC Impact === */\n    .nk-dynamic-checkout-block {\n      width: 100% !important;\n      max-width: 100% !important;\n      margin: 0 !important;\n      padding: 0 !important;\n      display: block;\n    }\n\n    /* Certains thèmes enveloppent le bloc Custom liquid avec un wrapper qui ajoute du padding max-width\n       -> on neutralise ces contraintes AU SEIN du bloc */\n    .nk-dynamic-checkout-block,\n    .nk-dynamic-checkout-block > * {\n      max-width: 100% !important;\n      box-sizing: border-box;\n    }\n\n    /* Conteneur injecté par Shopify */\n    .nk-dynamic-checkout .shopify-payment-button {\n      width: 100% !important;\n      max-width: 100% !important;\n      display: flex !important;\n      justify-content: center;\n      align-items: center;\n    }\n\n    /* Le bouton dynamique lui-même (Apple Pay / Acheter maintenant) */\n    .nk-dynamic-checkout .shopify-payment-button__button {\n      display: flex !important;\n      justify-content: center;\n      align-items: center;\n      width: 100% !important;\n      max-width: 100% !important;\n      min-height: 52px;           /* aligne la hauteur avec ATC */\n      border-radius: 9999px;      /* arrondi style Impact */\n      padding: 0 16px;\n      font-weight: 600;\n      line-height: normal;\n      box-sizing: border-box;\n    }\n\n    /* Optionnel : masquer \"Plus d’options de paiement\" pour garder un seul CTA pleine largeur */\n    .nk-dynamic-checkout .shopify-payment-button__more-options {\n      display: none !important;\n    }\n\n    /* Cas fréquent : certains thèmes mettent les enfants en inline-block => on force block/100% */\n    .product-form__buttons > .nk-dynamic-checkout,\n    .product-form__buttons > .nk-dynamic-checkout * {\n      width: 100% !important;\n      max-width: 100% !important;\n    }\n  </style>\n\n  <script>\n    (function() {\n      /* Synchronise la variante du bouton dynamique avec le sélecteur principal */\n      var mainVariantInput = document.querySelector('form[action*=\"/cart/add\"] input[name=\"id\"]');\n      var myVariantInput   = document.querySelector('#nk-dynamic-checkout input[name=\"id\"]');\n\n      function syncVariant() {\n        if (mainVariantInput && myVariantInput && mainVariantInput.value) {\n          myVariantInput.value = mainVariantInput.value;\n        }\n      }\n\n      if (mainVariantInput && myVariantInput) {\n        ['change','input'].forEach(function(evt){\n          mainVariantInput.addEventListener(evt, syncVariant, { passive: true });\n        });\n        var mo = new MutationObserver(syncVariant);\n        mo.observe(mainVariantInput, { attributes: true, attributeFilter: ['value'] });\n        syncVariant();\n      }\n    })();\n  </script>\n{% endif %}"
